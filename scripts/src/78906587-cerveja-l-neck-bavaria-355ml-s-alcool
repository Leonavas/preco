<script>
  var barcode_width = $('#barcode-width').slider().on('slide', generateBigCode);
  var barcode_height = $('#barcode-height').slider().on('slide', generateBigCode);
  var barcode_font_size = $('#barcode-font-size').slider().on('slide', generateBigCode);
  function generateBigCode(){
  	options = {format: 'EAN',
  		displayValue: true,
  		fontSize: barcode_font_size.val()*1,
  		width: barcode_width.val()*1,
  		height: barcode_height.val()*1
  	}
  
  	$("#barcode-big").JsBarcode("0000078906587",options);
  	$("#barcode-download").attr('href', $("#barcode-big").attr('src'));
  };
  generateBigCode();
</script>


