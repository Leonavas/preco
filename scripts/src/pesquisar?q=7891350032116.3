<div aria-labelledby='searchGpcModal' class='modal fade' id='search-gpc' role='dialog'>
<div class='modal-dialog' role='document'>
<div class='modal-content'>
<div class='modal-header'>
<button aria-label='Close' class='close' data-dismiss='modal' type='button'>
<span aria-hidden='true'>×</span>
</button>
<h4 class='modal-title'>Buscar GPC</h4>
</div>
<div class='modal-body'>
<div class='form'>
<div class='form-group'>
<label class='control-label' for='search_gpc'>GPC:</label>
<input id='search_gpc' name='change[gpc]'>
</div>
</div>
</div>
<div class='modal-footer'>
<button class='btn btn-default' data-dismiss='modal' type='button'>Cancelar</button>
<button class='btn btn-primary' id='btn-change-gpc' type='submit'>Alterar GPC</button>
</div>
</div>
</div>
</div>
</form>

<script>
  $("#search_gpc").select2({
      width: 350,
      placeholder: 'Informe o código do GPC',
      minimumInputLength: 4,
      initSelection : function (element, callback) {
            var data = {id: element.val(), description: element.data('description')};
            callback(data);
      },
      id: function(gpc){
          return gpc.id;
      },
      ajax: {
          url: "/gpcs/search_ajax.json",
          dataType: 'json',
          data: function (term, page) {
              return {
                  "search[search_term]": term,
              };
          },
          results: function (data, page) {
              return {results: data};
          }
      },
      formatResult: function(gpc) {
          return gpc.description;
      },
      formatSelection: function(gpc) {
          return gpc.description;
      }
  }).on("change", function(e) {
      reload_profiles(e.val);
  });
</script>

<form action="/products/change_ncm" accept-charset="UTF-8" data-remote="true" method="post"><input name="utf8" type="hidden" value="&#x2713;" />
<input id='ncm-product-id' name='change[products][]' type='hidden' value='276250'>
<div aria-labelledby='exampleModalLabel' class='modal fade' id='search-profile' role='dialog'>
<div class='modal-dialog' role='document'>
<div class='modal-content'>
<div class='modal-header'>
<button aria-label='Close' class='close' data-dismiss='modal' type='button'>
<span aria-hidden='true'>×</span>
</button>
<h4 class='modal-title'>Buscar NCM</h4>
</div>
<div class='modal-body'>
<div class='form'>
<div class='form-group'>
<label class='control-label' for='search_ncm'>NCM:</label>
<input data-description='3305.90.00 - Outras' id='search_ncm' name='change[ncm]' value='6590'>
</div>
<div class='form-group'>
<label class='control-label' for='search_federal_tax_profile_ex'>Figura Fiscal:</label>
<select id='search_federal_tax_profile_ex' name='change[federal_tax_profile_ex]'>
<option value="0">NCM 3305.90.00 (Principal) - </option>
<option value="1">NCM 3305.90.00 - Exceção: 1 - Tintura</option>
</select>
</div>
</div>
</div>
<div class='modal-footer'>
<button class='btn btn-default' data-dismiss='modal' type='button'>Cancelar</button>
<button class='btn btn-primary' id='btn-change-ncm' type='submit'>Alterar NCM</button>
</div>
</div>
</div>
</div>
<script>
      function apply_select2_profiles() {
        $("#search_federal_tax_profile_ex").select2({
          width: 350
        });
      }
  
      function remove_box_alerts(){
         $('#alert').hide();
         disabled_elements(false);
      }
  
      function none_federal_tax_profile(){
        $('#search_federal_tax_profile_ex').html("<option value='-1'>Nenhuma figura fiscal cadastrada</option>");
        apply_select2_profiles();
      }
  
      function disabled_elements(status){
        $('#btn-change-ncm').prop('disabled',status);
        $('#search_federal_tax_profile_ex').prop('disabled',status);
      }
  
      function add_box_alerts(ncm_id){
        none_federal_tax_profile();
        disabled_elements(true);
        $("a#link-alert").attr('href', '/federal_tax_profiles/new/' + ncm_id);
        $("a#link-alert").attr('target','_blank');
        $('#alert').show();
      }
  
      function reload_profiles(ncm_id) {
        remove_box_alerts();
  
        var url = '/ncms/'+ncm_id+'/profiles.json';
        var options;
        $.get(url).done(function(data){
            $.each(data, function(key,value){
              options = options + " <option value='"+value.federal_tax_profile_ex+"'>" +value.description+'</option>';
            })
  
          $('#search_federal_tax_profile_ex').html(options);
            if (options == undefined){
              add_box_alerts(ncm_id);
            }
            else{
              apply_select2_profiles();
              disabled_elements(false);
            }
        });
      }
  
  $(document).ready(function(){
      disabled_elements(true);
      remove_box_alerts();
  
      $("#search_ncm").select2({
          width: 350,
          placeholder: 'Informe o código do NCM',
          minimumInputLength: 4,
          initSelection : function (element, callback) {
                var data = {id: element.val(), description: element.data('description')};
                callback(data);
          },
          id: function(ncm){
              return ncm.id;
          },
          ajax: {
              url: "/ncms/search_ajax.json",
              dataType: 'json',
              data: function (term, page) {
                  return {
                      "search[search_term]": term,
                      "search[ncm_valid]": '2'
                  };
              },
              results: function (data, page) {
                  return {results: data};
              }
          },
          formatResult: function(ncm) {
              return ncm.description;
          },
          formatSelection: function(ncm) {
              return ncm.description;
          }
      }).on("change", function(e) {
          reload_profiles(e.val);
      });
  
      apply_select2_profiles();
  });
</script>

</form>

